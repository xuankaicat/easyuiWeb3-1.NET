@model ProductEditViewModel

<form id="ddform">
    <input id="HdId" hidden asp-for="Id" />
    <div id="divU">
        <table cellpadding="0" cellspacing="1" border="0" style="width: 100%;" bgcolor="b5d6e6">
            <tr style="background-color: White; height: 32px;">
                <td style="width: 80px; padding-left: 5px">
                    <label asp-for="产品名称"></label>
                </td>
                <td style="padding: 5px">
                    <input id="pname" type="text" asp-for="产品名称" class="easyui-validatebox" required style="border: 1px solid #8DB2E3; width: 200px; height: 20px" />
                </td>
            </tr>
            <tr style="background-color: White; height: 32px;">
                <td style="padding: 5px">
                    <label asp-for="产品类型"></label>
                </td>
                <td style="padding: 5px">
                    <input id="pkind" type="text" asp-for="产品类型" class="easyui-validatebox" required style="border: 1px solid #8DB2E3; width: 200px; height: 20px" />
                </td>
            </tr>
            <tr style="background-color: White; height: 32px;">
                <td style="padding: 5px">
                    <label asp-for="单位名称"></label>
                </td>
                <td style="padding: 5px">
                    <input id="pcompany" type="text" asp-for="单位名称" class="easyui-validatebox" required style="border: 1px solid #8DB2E3; width: 200px; height: 20px" />
                </td>
            </tr>
            <tr style="background-color: White; height: 32px;">
                <td style="padding: 5px">
                    <label asp-for="库存数量"></label>
                </td>
                <td style="padding: 5px">
                    <input id="pnum" class="easyui-numberspinner" type="text" asp-for="库存数量" min="0" max="99999999" required style="width: 80px;" />
                </td>
            </tr>
        </table>
    </div>
    <div border="false" style="text-align: right; height: 30px; line-height: 30px;padding-top:7px">
        <button type="button" class="easyui-linkbutton" icon="icon-ok" onclick="editEnd()">确定</button>
        <a id="A4" class="easyui-linkbutton" onclick='$("#dd2").dialog("close");' icon="icon-cancel" href="javascript:void(0)">
            取消
        </a>
    </div>
</form>